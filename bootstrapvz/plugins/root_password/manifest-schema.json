{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Root password plugin manifest",
	"type": "object",
	"properties": {
		"plugins": {
			"type": "object",
			"properties": {
				"root_password": {
					"type": "object",
					"properties": {
						"password": {
							"type": "string"
						}
					},
					"required": ["password"]
				}
			}
		}
	}
}
